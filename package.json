{
  "name": "star-orgs",
  "private": true,
  "version": "0.0.0",
  "description": "Azure Active Directory Organization Chart Viewer",
  "main": "server.js",
  "keywords": [
    "azure",
    "active directory",
    "ad"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ritterim/star-orgs"
  },
  "scripts": {
    "audit": "nsp check package",
    "build": "npm run build-client && npm run build-server",
    "build-client": "webpack -d",
    "build-client-production": "cross-env NODE_ENV=production webpack -d",
    "build-server": "babel src/server -d dist",
    "build-server-and-start-server": "npm run build-server && node ./server.js",
    "check": "npm run audit && npm outdated --depth 0",
    "clean:keep-env": "git clean -xdf --exclude .env",
    "coverage": "nyc --all --require babel-core/register --reporter=lcov --reporter=text --reporter=html ava",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "eslint src test",
    "start": "node ./server.js",
    "start:watch": "concurrently \"npm run watch-client\" \"npm run watch-server\"",
    "test": "ava",
    "validate": "npm run lint && npm test",
    "prepare": "npm run build && npm run validate && npm run check",
    "pp": "npm run prepare",
    "watch-client": "webpack -d --watch",
    "watch-server": "nodemon --exec npm run build-server-and-start-server --watch src/server --watch .env"
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./test/helpers/setup-browser-env.js",
      "./test/helpers/suppress-node-uuid-warning.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**"
    ]
  },
  "author": {
    "name": "Ritter Insurance Marketing"
  },
  "pre-commit": [
    "validate"
  ],
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-preset-es2015": "^6.9.0",
    "cache-storage": "^2.0.1",
    "chance": "^1.0.3",
    "compression": "^1.6.2",
    "css-loader": "^0.23.1",
    "d3": "^4.2.2",
    "d3-svg-legend": "^2.9.0",
    "dotenv": "^2.0.0",
    "eventemitter3": "^1.2.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "md5": "^2.1.0",
    "node-sass": "^3.8.0",
    "randgen": "^0.1.0",
    "request": "^2.73.0",
    "request-promise": "^3.0.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "whatwg-fetch": "^1.0.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "concurrently": "^2.1.0",
    "coveralls": "^2.11.9",
    "cross-env": "^2.0.0",
    "eslint": "^2.5.1",
    "fetch-mock": "4.5.2",
    "jsdom": "^9.3.0",
    "nodemon": "^1.9.2",
    "nsp": "^2.2.0",
    "nyc": "^6.1.1",
    "polyfill-crypto.getrandomvalues": "1.0.0",
    "pre-commit": "^1.1.2",
    "proxyquire": "^1.7.9",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": "5.9.x",
    "npm": "3.7.3"
  }
}
